{
  "manifestVersion": 1,
  "id": "vsts-marathonapi-build-tasks",
  "version": "1.0.0",
  "name": "Marathon API tasks",
  "description": "VSTS Build tasks which allow interactions with Mesos Marathon API.",
  "publisher": "CdiscountAlm",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "icons": {
    "default": "img/marathon.jpg"
  },
  "categories": [
    "Build and release"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/Cdiscount/vsts-buildtask-marathon"
  },
  "galleryFlags": [
    "Public"
  ],
  "tags": [
    "marathon",
    "mesos",
    "api",
    "docker"
  ],
  "files": [
    {
      "path": "Tasks/MarathonDeploy"
    }
  ],
  "contributions": [
    {
      "id": "vsts-marathonapi-build-marathondeploy",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/MarathonDeploy"
      }
    }
  ]
}